# Environment Variables Example for Web Frontend
# Copy this file to .env for development or production.env for production

# API Configuration
NEXT_PUBLIC_API_URL=http://127.0.0.1:80/api
NEXT_PUBLIC_SSR_API_URL=http://127.0.0.1:80/api
NEXT_PUBLIC_API_VERSION=v1

# WebSocket Configuration
NEXT_PUBLIC_WS_URL=ws://127.0.0.1:80/api/ws

# OAuth Configuration
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-client-id
NEXT_PUBLIC_OAUTH_REDIRECT_HOST=http://127.0.0.1:80/api

# Optional: Production Domain
NEXT_PUBLIC_DOMAIN=localhost
NEXT_PUBLIC_BASE_URL=http://127.0.0.1:80

# Next.js Configuration
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1
